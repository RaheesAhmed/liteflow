{
  "name": "create-liteflow-app",
  "version": "0.2.11",
  "description": "Create LiteFlow apps with one command - The Modern Web Framework",
  "type": "commonjs",
  "bin": {
    "create-liteflow-app": "./dist/index.js"
  },
  "files": [
