{
  "name": "create-liteflow",
  "version": "0.1.3",
  "description": "Create LiteFlow apps with one command",
  "bin": {
    "create-liteflow": "./bin/create-liteflow.cmd"
  },
  "files": [
    "dist",
    "templates/**",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node bin/create-liteflow.cmd",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist templates/basic",
    "prepublishOnly": "pnpm run build",
    "prepack": "node -e \"const fs=require('fs-extra');const path=require('path');fs.removeSync('templates/basic');fs.copySync('../../examples/basic',path.join('templates','basic'),{filter:src=>!src.includes('node_modules')})\""
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.6",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "liteflow",
    "create-liteflow",
    "react",
    "vite",
    "typescript",
    "tailwindcss",
    "web",
    "framework",
    "template",
    "starter"
  ],
  "author": "LiteFlow Team",
  "homepage": "https://liteflow.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liteflow/liteflow.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/liteflow/liteflow/issues"
  },
  "license": "MIT"
}
